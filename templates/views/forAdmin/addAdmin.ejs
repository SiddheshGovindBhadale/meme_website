<!DOCTYPE html>

<html>
    <head>
        <title>Add admin</title>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <!-- UNICONS -->
        <link rel="stylesheet" href="https://unicons.iconscout.com/release/v3.0.6/css/line.css">
        <!-- icon -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0/css/all.min.css"
        integrity="sha512-9usAa10IRO0HhonpyAIVpjrylPvoDwiPUiKdWk5t3PyolY1cOd4DSE0Ga+ri4AuTroPR5aQvXU9xC6qOPnzFeg=="
        crossorigin="anonymous" referrerpolicy="no-referrer" />
        <!-- BOXICONS -->
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">
        
        <!-- bootstrap -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
        
        <!-- style styling -->
        <link href="../css/styles.css" rel="stylesheet">
        <!-- services styling -->
        <link href="../css/services.css" rel="stylesheet">
        <!-- table styling -->
        <link href="../css/table.css" rel="stylesheet">
        <!-- loader css -->
        <link href="../css/loader.css" rel="stylesheet"  >
        
        
    </head>
    
    <style>
        *{
          margin:0px ;
          padding:0px;
          box-sizing:border-box;
        }
        
      
        .admins .form-wrap{ 
          max-width:400px;
          background: #fff; 
          box-sizing: border-box; 
          margin:50px auto;
          padding:30px 20px;
       }
       
        .admins h1{
          text-align: center; 
          color: #11406a; 
          font-weight:700;
          font-size:1.5rem;
          margin-bottom: 20px;
          text-transform:uppercase;
        }
        
        .admins input{
          width: 100%; 
          background: none; 
          border: 1px solid #11406a; 
          border-radius: 3px; 
          padding: 6px 15px; 
          box-sizing: border-box; 
          margin-bottom: 20px; 
          font-size: 16px; 
          color: #11406a;
        }
        
        .admins input[type="submit"]{ 
          background: #11406a; 
          border: 0; 
          cursor: pointer; 
          color: #fff;
          border: 1px solid #11406a; 
        }
        .admins input[type="submit"]:hover{ 
          background: #11406a; 
          transition: .6s;
        }
        
        .admins ::placeholder{
          color: #11406a;
        }
        
        #change{
         transition:all .3s;
        }
    
    </style>

    <body>
    <div id="message"></div>
    <!--loader-->
    <div id="mains">
        <div class="bg" >
             <div class="loader"></div>
        </div>
    </div>
    
    <header class="header">
    <a href="#">
    <img src="../img/logo/logo.png" alt="YouTube Logo" class="youtube-logo" />
    </a>
    <div class="search">
    <div class="searchbar">
    <input type="text" class="searchbar__input" name="q" placeholder="Search..." id="search">
    <a href="#table" id="searchBtn" class="searchbar__button">
    <i><img src="../img/icon/search.png"></i>
    </a>
    </div>
    </div>
    </header>
    
    <section class="admins container" >
        <div class="changeBtn">
             <button class="active" onclick="allStaff()">All Staff</button>
             <button onclick="addStaff()">Add Staff</button>
        </div>
        <div id="change" >
             <div class="table_responsive">
             <table>
             <thead>
             <tr>
             <th>No</th>
             <th>Image</th>
             <th>Name</th>
             <th>Username</th>
             <th>Admin</th>
             <th>Action</th>
             </tr>
             </thead>
             
             <tbody>
             
             
             </tbody>
             </table>
             </div>
        </div>
    </section>
    <!-- loader js -->
    <script src="../js/loader.js"></script>
    <!-- add Staff js -->
    <script src="../js/addAdmin.js"></script>
    
    </body>



</html>