<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
        <!-- services styling -->
        <link href="../css/services.css" rel="stylesheet">
        <!-- table styling -->
        <link href="../css/table.css" rel="stylesheet">
        <!-- loader css -->
        <link href="../css/loader.css" rel="stylesheet"  >
        <link rel="stylesheet" href="../css/styles.css">
<title>Upload Video</title>
<style type="text/css">

#addProduct .product-card{
  border:1px solid #d1d4d7;
  border-radius:5px;
  margin:0px;
  padding:10px;
  box-shadow:2px 2px 3px #d1d4d7;
}

#addProduct .product-card .imag{
  border:1px solid #d1d4d7;
  border-radius:5px;
  height:200px;
  display:flex;
  align-items:center;
  justify-content:center;
  padding:10px;
}

#upload {
  width:100%;
}

form {
  max-width:400px;
  margin:0 auto;
  background:#fff;
}
</style>
</head>
<body>
    <div id="message"></div>
    <!--loader-->
    <div id="mains">
        <div class="bg" >
             <div class="loader"></div>
        </div>
    </div>

<header class="header">
    <a href="#">
      <img src="../img/logo/logo.png" alt="YouTube Logo" class="youtube-logo" />
    </a>
    <div class="search">
         <div class="searchbar">
              <input type="text" class="searchbar__input" name="q" placeholder="Search..." id="search">
              <a href="#table" id="searchBtn" class="searchbar__button">
                   <i><img src="../img/icon/search.png"></i>
              </a>
         </div>
    </div>
  </header>
    
<section class="admins container" >
      <div class="changeBtn">
           <button class="active" onclick="allVideo()">All Videos</button>
           <button onclick="addVideo()">Add Video</button>
      </div>
      <div id="change" >
           <div class="table_responsive">
                <table>
                      <thead>
                            <tr>
                                <th>No</th>
                                <th>Date</th>
                                <th>Thumbnail</th>
                                <th>Title</th>
                                <th>Category</th>
                                <th>Keys</th>
                                <th>Play Time</th>
                                <th>Discretion</th>
                                <th>Action</th>
                            </tr>
                      </thead> 
                      <tbody>
                
                      </tbody>
               </table>
           </div>
      </div>
</section>

    <!-- loader js -->
    <script src="../js/loader.js"></script>
    <!-- add Staff js -->
    <script src="../js/addVideo.js"></script>
    
</body>
</html>